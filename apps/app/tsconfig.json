{
  "exclude": ["node_modules"],
  "extends": "@bubble/tsconfig/nextjs.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./renderer/src/*"
      ],
      "@configs/*": [
        "./renderer/configs/*"
      ]
    }
  },
  "include": ["src", "next-env.d.ts"]
}
