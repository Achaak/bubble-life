{
  "name": "@bubble/storybook",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoi@gmail.com>",
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx,json}\" --fix",
    "start": "http-server ./storybook-static",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@bubble/styles": "workspace:*",
    "@bubble/translate": "workspace:*",
    "@bubble/ui": "workspace:*",
    "http-server": "^14.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "devDependencies": {
    "@bubble/scripts": "workspace:*",
    "@bubble/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-storyshots": "^6.5.10",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/cli": "^6.5.10",
    "@storybook/components": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.10",
    "@types/node": "^18.7.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
