{
  "name": "@bubble/storybook",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoi@gmail.com>",
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx,json}\" --fix",
    "start": "http-server ./storybook-static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@bubble/styles": "workspace:*",
    "@bubble/translate": "workspace:*",
    "@bubble/ui": "workspace:*",
    "http-server": "^14.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@bubble/scripts": "workspace:*",
    "@bubble/tsconfig": "workspace:*",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-storyshots": "^6.4.21",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/cli": "^6.4.21",
    "@storybook/components": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@storybook/theming": "^6.4.21",
    "@types/node": "^17.0.23",
    "@types/react": "18.0.2",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "cross-env": "^7.0.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "prettier": "2.6.2",
    "typescript": "4.5.5"
  }
}
