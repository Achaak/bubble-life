{
  "name": "@bubble/storybook",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoie@gmail.com>",
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx}\" --fix",
    "start": "http-server ./storybook-static",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@bubble/styles": "workspace:*",
    "@bubble/translate": "workspace:*",
    "@bubble/ui": "workspace:*",
    "http-server": "^14.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@bubble/scripts": "workspace:*",
    "@bubble/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-storyshots": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.15",
    "@storybook/addons": "6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/cli": "^6.5.15",
    "@storybook/components": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/theming": "^6.5.15",
    "@types/react": "^18.0.26",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.31.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  }
}
