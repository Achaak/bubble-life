{
  "name": "@bubble/docs",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoie@gmail.com>",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "build:docker": "cd ../../ && docker build . -f dockerfiles/website.Dockerfile -t docs:latest --build-arg SCOPE=@bubble/docs",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,.md}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx}\" --fix",
    "start": "next start",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@bubble/styles": "workspace:*",
    "@bubble/ui": "workspace:*",
    "next": "13.1.1",
    "nextra": "2.0.3",
    "nextra-theme-docs": "2.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@bubble/scripts": "workspace:*",
    "@bubble/tsconfig": "workspace:*",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.26",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "next-transpile-modules": "^10.0.0",
    "prettier": "^2.8.1",
    "typescript": "4.9.4"
  }
}
