{
  "name": "@bubble/docs",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoi@gmail.com>",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "build:docker": "cd ../../ && docker build . -f dockerfiles/website.Dockerfile -t docs:latest --build-arg SCOPE=@bubble/docs",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,.md}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx,json}\" --fix",
    "start": "next start",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@bubble/ui": "workspace:*",
    "next": "12.2.5",
    "nextra": "^2.0.0-alpha.48",
    "nextra-theme-docs": "^2.0.0-alpha.48",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@bubble/scripts": "workspace:*",
    "@bubble/tsconfig": "workspace:*",
    "@next/eslint-plugin-next": "12.2.5",
    "@types/node": "^18.7.3",
    "@types/react": "18.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}
