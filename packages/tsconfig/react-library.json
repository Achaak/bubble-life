{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "target": "ES6",
    "rootDir": "src",
    "outDir": "dist",
    "jsx": "preserve"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
