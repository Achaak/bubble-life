{
  "name": "@bubble/translate",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoie@gmail.com>",
  "main": "./src/index.ts",
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "i18n-watch": "typesafe-i18n",
