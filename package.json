{
  "name": "bubble-life",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev-interface\" \"yarn dev-server\"",
    "build": "yarn build-interface && yarn build-server",
    "start": "concurrently \"yarn start-interface\" \"yarn start-server\"",
    "dev-interface": "cd interface && yarn dev",
    "build-interface": "cd interface && yarn build",
    "start-interface": "cd interface && yarn start",
    "dev-server": "cd server && yarn dev",
    "build-server": "cd server && yarn build",
    "start-server": "cd server && yarn start",
    "i": "yarn && yarn i-interface && yarn i-server",
    "i-interface": "cd interface && yarn",
    "i-server": "cd server && yarn"
  },
  "repository": "https://github.com/Achaak/bubble-life.git",
  "author": "Axel Lavoie <axel.lavoie@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.3.0"
  }
}
