{
  "name": "bubble-life",
  "description": "Bubble-life is a NodeJs project for various device. He can inform you of the weather, the time or your calendar. All this with Bubble who will live this life according to your modules.",
  "version": "1.0.0",
  "author": "Axel Lavoie <axel.lavoi@gmail.com>",
  "main": "app/background.js",
  "scripts": {
    "start": "set NODE_ENV=production && nextron",
    "dev": "nextron",
    "build": "nextron build",
    "build:all": "nextron build --all",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "build:mac": "nextron build --mac --x64",
    "build:linux": "nextron build --linux",
    "postinstall": "electron-builder install-app-deps",
    "i18n": "typesafe-i18n",
    "i18n:generate": "tsm i18n-generate.ts",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "format": "prettier --write \"./**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "animejs": "^3.2.1",
    "dayjs": "^1.11.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "react-fps-stats": "^0.2.3",
    "react-redux": "^7.2.6",
    "react-spinners": "^0.11.0",
    "react-toast-notifications": "^2.5.1",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "shortid": "^2.2.16",
    "typesafe-i18n": "^4.4.2"
  },
  "devDependencies": {
    "@stitches/react": "^1.2.7",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/animejs": "^3.1.4",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-toast-notifications": "^2.4.1",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "next": "^12.1.0",
    "nextron": "^8.0.0",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsm": "^2.2.1",
    "typescript": "^4.6.2"
  },
  "packageManager": "yarn@3.0.2"
}
